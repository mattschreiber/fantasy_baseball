<table table- class="tablesorter-blue team-projection">
	 <tr class="team-project-totals">
      <td colspan="4"></td>
      <td>Totals:</td>
      <td><%= pitcher_totals[:so]%></td>
      <td><%= pitcher_totals[:wins]%></td>
